import pandas as pd
import numpy as np
#Создадим Series
a = pd.Series([1, 2, 3, 4, 5])
b = pd.Series([6, 7, 8, 9, 10])

#Евклидово расстояние
d = np.sqrt(np.sum(a - b)**2) #считаем сумму разностей элементов в квадрате, по формуле Евклида

print("Евклидово расстояние между двумя Series (точками) a и b: ", d)
